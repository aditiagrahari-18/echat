<div class="col-xs-12 col-md-12 div_frends_list overflow-auto msglist"  >
  <% @chatlist.where("sender_id =? or reciever_id =?", user.id, user.id).each do |msg| %>
    <div class="col-xs-12 col-md-12 margin-left">
      <% if msg.reciever_id == current_user.id && msg.sender_id == user.id && msg.msg_body != ""  %>
           <p class="msgboxreciever"><%=  "[ #{msg.created_at.strftime("%I:%M %p")}]"  %>
           <% user.first_name ||= user.username  %>
           <%= "#{msg.msg_body}" %></p>
      <% end %>
    </div>
  <div class="col-xs-12 col-md-12 margin-right">
    <% if msg.reciever_id == user.id && msg.sender_id == current_user.id && msg.msg_body != ""  %>
         <p class="msgboxsender"><%=  " [ #{msg.created_at.strftime("%I:%M %p")}]" %>
      <% current_user.first_name ||= current_user.username  %>
      <%= "#{msg.msg_body}" %></p>
    <% end %>
  </div>
  <% end %>
</div>
